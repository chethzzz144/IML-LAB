from datetime import datetime

class Dog:
    def __init__(self, name, size, breed="Unknown", dob="Unknown"):
        self.name, self.size, self.breed, self.dob = name, size, breed, dob

    def bark(self):
        print("Woof!")

    def get_name(self):
        return self.name

    def set_name(self, new_name):
        self.name = new_name.title() if 2 <= len(new_name) <= 30 and new_name.isalpha() else (
            print("Invalid name. Please provide an alphabetical name between 2 and 30 characters."), self.name)[1]

    def dog_years(self):
        return int(((datetime.today() - datetime.strptime(self.dob, "%d/%m/%Y")).days / 365.25 * 7)
                   if self.dob != "Unknown" else (
                       print("Dog's date of birth is unknown. Unable to calculate dog years."), 0)[1])

# Example usage:
my_dog = Dog(name="Buddy", size="Medium", breed="Labrador", dob="10/05/2018")

my_dog.bark()
print(f"Dog's name: {my_dog.get_name()}")

my_dog.set_name("Max")
print(f"Updated dog's name: {my_dog.get_name()}")

print(f"Dog's age in dog years: {my_dog.dog_years()} years")
