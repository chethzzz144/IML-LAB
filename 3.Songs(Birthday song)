class Song:
    def __init__(self, lyrics):
        self.lyrics = lyrics

    def sing_me_a_song(self):
        for line in self.lyrics:
            print(line)

happy_bday_lyrics = [
    "May God bless you,",
    "Have sunshine on you,",
    "Happy Birthday to you!"
]

happy_bday = Song(happy_bday_lyrics)

happy_bday.sing_me_a_song()
